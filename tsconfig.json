{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES6",
    "module": "ESNext",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node",
      "cypress",
      "cypress-file-upload",
      "cypress-real-events"
    ],
    "paths": {
      "cypress/*": [
        "./cypress/*"
      ]
    },
  },
  "include": [
    "cypress/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}